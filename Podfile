# Uncomment this line to define a global platform for your project
platform :ios, '9.0'
swift_version = '3.2'
target 'Whatisit' do
    # Comment this line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    pod 'Firebase/Core'
    pod 'Firebase/AdMob'
    pod 'SwiftyJSON'
    pod 'Realm'
    pod 'RealmSwift'
    # Pods for Whatisit
    
    target 'WhatisitTests' do
        inherit! :search_paths
        # Pods for testing
    end
    
    target 'WhatisitUITests' do
        inherit! :search_paths
        # Pods for testing
    end
    
end
